<odoo>
    <data>
        <record id="deped_principals_action" model="ir.actions.act_window">
            <field name="name">Principals</field>
            <field name="res_model">deped.principal</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="deped_principals_view_tree" model="ir.ui.view">
            <field name="name">Principals List</field>
            <field name="model">deped.principal</field>
            <field name="arch" type="xml">
                <tree string="principals">
                    <field name="teacher_id" />
                    <field name="school_id" />
                    <field name="start_date" />
                    <field name="end_date" />
                </tree>
            </field>
        </record>
        <record id="deped_principals_view_form" model="ir.ui.view">
            <field name="name">principal</field>
            <field name="model">deped.principal</field>
            <field name="arch" type="xml">
                <form string="Test">
                    <sheet>
                        <group>
                            <field name="school_id" />
                            <field name="teacher_id" />
                            <field name="start_date" />
                            <field name="end_date" />
                        </group>
                    </sheet>   
                </form>
            </field>
        </record>
    </data>
</odoo>